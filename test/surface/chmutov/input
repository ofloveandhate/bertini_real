CONFIG

tracktype:1;

condnumthreshold:1e100;
singvalzerotol:1e-30;

securitymaxnorm:1e8;
tracktolbeforeeg: 1e-6;
odepredictor:7;
printpathmodulus:100;
numsamplepoints:3;
sharpendigits:30;

